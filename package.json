{
  "name": "redux-openfin",
  "version": "0.10.01.beta",
  "description": "redux middleware for openfin in typescript",
  "main": "index.js",
  "scripts": {
    "build":"yarn prebuild && yarn build:es2015 && yarn build:copy-files",
    "prebuild":"rimraf build",
    "build:es2015":"tsc src/index.ts --outDir build -d --declarationDir build",
    "build:copy-files":"node ./scripts/copy-files.js",
    "release":"yarn build && npm publish build --register=https://albertli.ddns.net:9004/repository/npm-private/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "redux": "^4.0.0",
    "redux-actions": "^2.4.0"
  },
  "devDependencies": {
    "@types/redux-actions": "^2.3.0",
    "copyfiles": "^2.0.0",
    "fs-extra": "^6.0.1",
    "rimraf": "^2.6.2"
  }
}
